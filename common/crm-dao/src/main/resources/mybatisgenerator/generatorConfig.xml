<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<properties resource="mybatisgenerator/generatorConfig.properties"></properties>
	<!--<classPathEntry location="/app/.m2/repository/mysql/mysql-connector-java/5.1.30/mysql-connector-java-5.1.30.jar"/>-->
	
	<!-- data base set configure -->
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<plugin type="com.cjlu.crm.util.MybatisPlugin"></plugin>
		<commentGenerator>
			<property name="suppressDate" value="true"/>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true"/>
		</commentGenerator>
		
		<!--mysql数据库链接URL，用户名、密码-->
		<jdbcConnection driverClass="${jdbc.driverClass}" connectionURL="${jdbc.connectionURL}"
		                userId="${jdbc.userId}" password="${jdbc.password}">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>
		<!-- 生成模型的包名和位置-->
		<javaModelGenerator targetPackage="${package.do}"
		                    targetProject="${target.project}/src/main/java/">
			<property name="enableSubPackages" value="true"/>
			<property name="trimStrings" value="true"/>
			<property name="useActualColumnNames" value="true"/>
		</javaModelGenerator>
		<!-- 生成映射文件的包名和位置-->
		<sqlMapGenerator targetPackage="${package.mapper}"
		                 targetProject="${target.project}/src/main/resources/">
			<property name="enableSubPackages" value="true"/>
		</sqlMapGenerator>
		<!-- 生成DAO的包名和位置-->
		<javaClientGenerator type="XMLMAPPER" targetPackage="${package.dao}"
		                     targetProject="${target.project}/src/main/java/">
			<property name="enableSubPackages" value="true"/>
		</javaClientGenerator>
		
		<!-- 要生成哪些表-->

		<!--<table tableName="crm_chance"
		    domainObjectName="CrmChance"
		    enableUpdateByExample="false"
		    enableUpdateByPrimaryKey="false"
		    enableDeleteByExample="false"
		    enableDeleteByPrimaryKey="false"
		    enableCountByExample="false"
		    enableSelectByPrimaryKey="true"
			enableSelectByExample="false"
			enableInsert="false">
			<property name="useActualColumnNames" value="false"/>
		</table>

		<table tableName="crm_products"
			   domainObjectName="CrmProducts"
			   enableUpdateByExample="false"
			   enableUpdateByPrimaryKey="false"
			   enableDeleteByExample="false"
			   enableDeleteByPrimaryKey="false"
			   enableCountByExample="false"
			   enableSelectByPrimaryKey="true"
			   enableSelectByExample="false"
			   enableInsert="false">
			<property name="useActualColumnNames" value="false"/>
		</table>

		<table tableName="crm_customer"
			   domainObjectName="CrmCustomer"
			   enableUpdateByExample="false"
			   enableUpdateByPrimaryKey="false"
			   enableDeleteByExample="false"
			   enableDeleteByPrimaryKey="false"
			   enableCountByExample="false"
			   enableSelectByPrimaryKey="true"
			   enableSelectByExample="false"
			   enableInsert="false">
			<property name="useActualColumnNames" value="false"/>
		</table>-->
		<table tableName="crm_activity"
			   domainObjectName="CrmActivity"
			   enableUpdateByExample="false"
			   enableUpdateByPrimaryKey="false"
			   enableDeleteByExample="false"
			   enableDeleteByPrimaryKey="false"
			   enableCountByExample="false"
			   enableSelectByPrimaryKey="true"
			   enableSelectByExample="false"
