#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_webpack-cli@2.0.13@webpack-cli/bin/webpack.js" "$@"
  ret=$?
else 
  node  "$basedir/../_webpack-cli@2.0.13@webpack-cli/bin/webpack.js" "$@"
  ret=$?
fi
exit $ret
